N = int(input("Enter the positive integer N: "))
numbers = []

for i in range(N):
    number = int(input("Enter number {}: ".format(i+1)))
    numbers.append(number)

X = int(input("Enter the integer X: "))
if X in numbers:
    index = numbers.index(X) + 1
    print(str(index))
else:
    print("-1")
